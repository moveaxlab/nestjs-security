{
  "name": "@moveax/nestjs-security",
  "version": "1.0.0",
  "description": "A NestJS library to handle JWT authentication for web and mobile apps.",
  "authors": [
    "Michelle Laurenti <michelle.laurenti@moveax.it>"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:cjs": "npm-run-all build:cjs:*",
    "build:cjs:code": "nest build --config ./nest-cli.json",
    "build:esm": "npm-run-all build:esm:*",
    "build:esm:code": "nest build --config ./nest-cli-esm.json",
    "build:esm:rename": "renamer --path-element ext --find .js --replace .mjs \"./esm/**\"",
    "fix": "npm-run-all fix:*",
    "fix:lint": "eslint --ext .js,.ts --fix .",
    "fix:format": "prettier --write . ",
    "test": "npm-run-all test:*",
    "test:types": "tsc -p tsconfig.json",
    "test:lint": "eslint --ext .js,.ts .",
    "test:format": "prettier --check .",
    "test:unit": "jest"
  },
  "peerDependencies": {
    "@fastify/cookie": "^9.3.1",
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-fastify": "^10.3.2",
    "ioredis": "^5.3.2"
  },
  "dependencies": {
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "moment": "^2.30.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@fastify/cookie": "^9.3.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-fastify": "^10.3.2",
    "@nestjs/testing": "^10.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "renamer": "^5.0.0",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.2",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
